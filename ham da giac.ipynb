    "import turtle as t \n",
    "pen = t.Turtle()\n",
    "def draw(a,b=100):\n",
    "    angle = (a-2) *180 / a\n",
    "    for i in range(a):\n",
    "        t.fd(b)\n",
    "        t.lt(180 - angle)\n",
    "    t.done()\n",
    "    \n",
    "draw(6)\n",
    "        "
